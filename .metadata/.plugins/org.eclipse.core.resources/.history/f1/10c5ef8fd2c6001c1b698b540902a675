package rectangle;

public class TestRectangle {

	public static void main(String[] args) {
		Rectangle  obj1 = new Rectangle(1, 1);
		obj1.input();
		obj1.calculateArea();
		obj1.display();
		System.out.println("\n");
		Rectangle  obj2 = new Rectangle(1, 1);
		obj2.input();
		obj2.calculateArea();
		obj2.display();
		System.out.println("\n");
		Rectangle  obj3 = new Rectangle(1, 1);
		obj3.input();
		obj3.calculatePerimeter();
		obj3.display();
		System.out.println("\n");
		Rectangle  obj4 = new Rectangle(0, 0);
		obj4.input();
		obj4.calculatePerimeter();
		obj4.display();
		System.out.println("\n");
		Rectangle  obj5 = new Rectangle(0, 0);
		obj5.input();
		obj5.calculatePerimeter();
		obj5.display();
		System.out.println("\n");
		System.out.println("execution completed");
	}

}
